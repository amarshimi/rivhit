<?xml version="1.0" ?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
	<route method="POST" url="/V1/rivhit-rivhit/getproduct">
		<service class="Rivhit\Rivhit\Api\GetProductManagementInterface" method="postGetProduct"/>
		<resources>
			<resource ref="anonymous"/>
		</resources>
	</route>
	<route method="GET" url="/V1/rivhit-rivhit/getallproducts">
		<service class="Rivhit\Rivhit\Api\GetAllProductsManagementInterface" method="getGetAllProducts"/>
		<resources>
			<resource ref="anonymous"/>
		</resources>
	</route>
</routes>
